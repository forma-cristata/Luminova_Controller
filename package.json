{
  "name": "luminova",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "dev": "expo start --localhost --go",
    "prebuild": "expo prebuild",
    "devc": "npx expo start -c",
    "devclear": "npx expo start --clear",
    "devtunnel": "npx expo start --tunnel",
    "format": "npx @biomejs/biome format --write",
    "format-lint": "npx @biomejs/biome lint --write && npx @biomejs/biome format --write",
    "format-lint-org": "npx @biomejs/biome check --write",
    "unsafeFix": "npx biome check --write --unsafe"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.3.25",
    "caniuse-lite": "^1.0.30001735",
    "expo": "54.0.6",
    "expo-audio": "~1.0.11",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@biomejs/biome": "2.1.4",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.11",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [],
        "listUnknownPackages": false
      }
