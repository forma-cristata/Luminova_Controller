{
	"name": "react-code",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start --dev-client",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"test": "jest --watchAll",
		"lint": "expo lint",
		"dev": "expo start --localhost --go",
		"prebuild": "expo prebuild",
		"devc": "npx expo start -c",
		"devclear": "npx expo start --clear",
		"devtunnel": "npx expo start --tunnel",
		"format": "npx @biomejs/biome format --write",
		"format-lint": "npx @biomejs/biome lint --write",
		"format-lint-org": "npx @biomejs/biome check --write",
		"unsafeFix": "npx biome check --write --unsafe"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@expo/vector-icons": "^14.1.0",
		"@react-native-async-storage/async-storage": "^2.1.2",
		"@react-native-community/slider": "^4.5.6",
		"@react-native-picker/picker": "2.11.1",
		"@react-navigation/bottom-tabs": "^7.2.0",
		"@react-navigation/native": "^7.0.14",
		"@types/react-native-snap-carousel": "^3.8.11",
		"expo": "53.0.13",
		"expo-blur": "~14.1.5",
		"expo-camera": "~16.1.9",
		"expo-constants": "~17.1.5",
		"expo-contacts": "~14.2.5",
		"expo-dev-client": "~5.2.2",
		"expo-font": "~13.3.1",
		"expo-haptics": "~14.1.4",
		"expo-linear-gradient": "~14.1.5",
		"expo-linking": "~7.1.5",
		"expo-router": "~5.1.1",
		"expo-sensors": "~14.1.4",
		"expo-splash-screen": "~0.30.9",
		"expo-status-bar": "~2.2.3",
		"expo-symbols": "~0.4.5",
		"expo-system-ui": "~5.0.9",
		"expo-web-browser": "~14.2.0",
		"picker": "^0.1.4",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-native": "0.79.4",
		"react-native-async-storage": "^0.0.1",
		"react-native-ble-plx": "^3.5.0",
		"react-native-gesture-handler": "~2.24.0",
		"react-native-linear-gradient": "^2.8.3",
		"react-native-picker": "^4.3.7",
		"react-native-reanimated": "~3.17.4",
		"react-native-reanimated-carousel": "^4.0.2",
		"react-native-safe-area-context": "5.4.0",
		"react-native-screens": "~4.11.1",
		"react-native-snap-carousel": "^1.3.1",
		"react-native-web": "^0.20.0",
		"react-native-webview": "13.13.5"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/plugin-proposal-export-namespace-from": "^7.18.9",
		"@biomejs/biome": "2.1.4",
		"@types/jest": "^29.5.12",
		"@types/react": "~19.0.10",
		"jest": "^29.2.1",
		"jest-expo": "~53.0.7",
		"typescript": "^5.3.3"
	},
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"exclude": [
					"react-native-snap-carousel"
